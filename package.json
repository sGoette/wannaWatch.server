{
  "name": "wannawatch.server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev:frontend": "webpack serve --mode development",
    "dev:backend": "nodemon --watch backend --ext ts --exec \"node --loader ts-node/esm backend/server.ts\"",
    "dev": "concurrently -n FRONTEND,BACKEND -c blue,green \"npm run dev:frontend\" \"npm run dev:backend\"",
    "typecheck": "tsc --noEmit",
    "build:frontend": "cd frontend && webpack",
    "build:backend": "node scripts/build-backend.mjs",
    "build": "rm -rf dist && npm run typecheck && npm run build:frontend && npm run build:backend",
    "start": "node dist/backend/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fastify/static": "^8.3.0",
    "@fastify/websocket": "^11.2.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "axios": "^1.13.2",
    "fastify": "^5.6.2",
    "mime-types": "^3.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "react-router-dom": "^7.11.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/mime-types": "^3.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "esbuild": "^0.27.2",
    "html-webpack-plugin": "^5.6.5",
    "nodemon": "^3.1.11",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
